{
  "name": "web-components-tutorial-practical-web-components",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/web-components/tutorial/practical-web-components/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/web-components/tutorial/practical-web-components",
        "compiler": "babel",
        "main": "packages/web-components/tutorial/practical-web-components/src/main.ts",
        "tsConfig": "packages/web-components/tutorial/practical-web-components/tsconfig.app.json",
        "webpackConfig": "packages/web-components/tutorial/practical-web-components/webpack.config.js",
        "assets": [
          "packages/web-components/tutorial/practical-web-components/src/favicon.ico",
          "packages/web-components/tutorial/practical-web-components/src/assets"
        ],
        "index": "packages/web-components/tutorial/practical-web-components/src/index.html",
        "baseHref": "/",
        "styles": [
          "packages/web-components/tutorial/practical-web-components/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "fileReplacements": [
            {
              "replace": "packages/web-components/tutorial/practical-web-components/src/environments/environment.ts",
              "with": "packages/web-components/tutorial/practical-web-components/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/webpack:dev-server",
      "options": {
        "buildTarget": "web-components-tutorial-practical-web-components:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "web-components-tutorial-practical-web-components:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/web-components/tutorial/practical-web-components/**/*.ts"
        ]
      }
    }
  }
}
