{
  "name": "lang-scss",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/lang/scss/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/lang/scss",
        "compiler": "babel",
        "main": "packages/lang/scss/src/main.ts",
        "tsConfig": "packages/lang/scss/tsconfig.app.json",
        "webpackConfig": "packages/lang/scss/webpack.config.js",
        "assets": [
          "packages/lang/scss/src/favicon.ico",
          "packages/lang/scss/src/assets"
        ],
        "index": "packages/lang/scss/src/index.html",
        "baseHref": "/",
        "styles": ["packages/lang/scss/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "fileReplacements": [
            {
              "replace": "packages/lang/scss/src/environments/environment.ts",
              "with": "packages/lang/scss/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/webpack:dev-server",
      "options": {
        "buildTarget": "lang-scss:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "lang-scss:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/lang/scss/**/*.ts"]
      }
    }
  }
}
