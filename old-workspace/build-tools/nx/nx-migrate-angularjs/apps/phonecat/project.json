{
  "projectType": "application",
  "sourceRoot": "apps/phonecat/src",
  "prefix": "nx-migrate-pmjs",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/phonecat",
        "index": "apps/phonecat/src/index.html",
        "main": "apps/phonecat/src/main.ts",
        "polyfills": "apps/phonecat/src/polyfills.ts",
        "tsConfig": "apps/phonecat/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/phonecat/src/favicon.ico",
          "apps/phonecat/src/assets",
          "apps/phonecat/src/app"
        ],
        "styles": ["apps/phonecat/src/styles.scss"],
        "scripts": [
          "./node_modules/jquery/dist/jquery.min.js",
          "./node_modules/bootstrap/dist/js/bootstrap.min.js"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/phonecat/src/environments/environment.ts",
              "with": "apps/phonecat/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "phonecat:build:production"
        },
        "development": {
          "browserTarget": "phonecat:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "phonecat:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/phonecat/**/*.ts", "apps/phonecat/**/*.html"]
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "apps/phonecat/src/test.ts",
        "tsConfig": "apps/phonecat/tsconfig.spec.json",
        "karmaConfig": "apps/phonecat/karma.conf.js",
        "polyfills": "apps/phonecat/src/polyfills.ts",
        "styles": [],
        "scripts": [],
        "assets": []
      }
    }
  },
  "tags": []
}
